mixin catalogue-head()
  +b.catalogue-head&attributes(attributes)
    .container
      +e.P.title ΝΕΕΣ ΑΦΙΞΕΙΣ

      +e.P.results.show-lg (134  προϊόντα)

      +e.links.hide-lg
        each link in ['Φορέματα', 'Πουκάμισα', 'Ζακέτες/ Μπολερό', 'Μπλούζες']
          +e.A.link(href="#")!= link

      +e.A.back(href="#").show-lg
        +icon('arrow_right_burger')

      +e.texts.hide-lg
        +e.text.is-active-passive.is-active
