    +e.background
      img(src="assets/images/catalogue/header.jpg" alt="@@")

mixin ui-kit-catalogue-head()
  +ui-section
    +ui-section-head
      +ui-title Catalogue Header
      +ui-path
        p Component: components/catalogue-head/catalogue-head

    +ui-section-content(true)
      +catalogue-head

    //- With +ui-section-content(true) container disappears
    //- Choose +ui-elements-list() for creating list of inline elements
    //- Border for special section: +ui-special()
